<Files "protect-files">
  Order allow,deny
  Deny from all
</Files>

Header always set Content-Security-Policy "default-src 'self' 'unsafe-inline' 'unsafe-eval'; font-src 'self' https://fonts.gstatic.com:* https://stackpath.bootstrapcdn.com:* https://cdn.jsdelivr.net:*; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com:* https://stackpath.bootstrapcdn.com:* https://cdn.jsdelivr.net:*"

